<mvc:View controllerName="ui.provacap.controller.Home"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns:core="sap.ui.core"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <Table id="idProductsTable"
		inset="false"
		items="{
			path: 'Prodotti>/'
		}">
        
		<columns>
			<Column>
				<Text text="ID" />
			</Column>
			<Column>
				<Text text="Nome" />
			</Column>
			<Column>
				<Text text="Data" />
			</Column>
		</columns>
		<items>
			<ColumnListItem vAlign="Middle">
				<cells>
					<Text text="{path:'Prodotti>id'}"></Text>
                    <Text text="{path:'Prodotti>name'}"></Text>
                    <Text text="{path:'Prodotti>date'}"></Text>
				</cells>
			</ColumnListItem>
		</items>
	</Table>
    </Page>
</mvc:View>
